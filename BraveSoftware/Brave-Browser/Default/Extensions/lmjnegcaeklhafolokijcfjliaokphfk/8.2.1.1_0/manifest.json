{
   "action": {
      "default_icon": {
         "40": "content/images/icon-40.png"
      },
      "default_popup": "content/popup.html?panel=main",
      "default_title": "Video DownloadHelper"
   },
   "author": "Michel Gutierrez",
   "background": {
      "service_worker": "background/main.js"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Alt+Up",
            "mac": "Ctrl+Shift+Up"
         }
      },
      "default-action": {
         "description": "Execute default action",
         "suggested_key": {
            "default": "Alt+Down",
            "mac": "Ctrl+Shift+Down"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "injected/downloadhelper.net.js" ],
      "matches": [ "*://*.downloadhelper.net/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en_US",
   "description": "__MSG_appDesc__",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "content/images/icon-128.png",
      "32": "content/images/icon-32.png",
      "40": "content/images/icon-40.png",
      "48": "content/images/icon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "options_ui": {
      "open_in_tab": true,
      "page": "content/settings.html?panel=settings"
   },
   "permissions": [ "tabs", "contextMenus", "nativeMessaging", "webRequest", "downloads", "webNavigation", "notifications", "scripting", "storage", "declarativeNetRequest" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "8.2.1.1"
}
